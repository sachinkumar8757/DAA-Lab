
public class Student {
	private String USN,NAME,BRANCH,PHONE;
	Student(String usn,String name,String branch,String phone)
	{
		this.USN=usn;
		this.NAME=name;
		this.BRANCH=branch;
		this.PHONE=phone;
	}
	void display()
	{
		System.out.println(USN+"\t"+NAME+"\t"+BRANCH+"\t"+PHONE);
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub
          Student s1=new Student("1js","sachin","cse","8757");
          Student s2=new Student("1is","mukul","ise","123");
          System.out.println("Details of 1st Students");
          System.out.println("usn name branch phone");
          s1.display();
          System.out.println("Details of 2nd Students");
          System.out.println("usn name branch phone");
          s2.display();
	}
}
